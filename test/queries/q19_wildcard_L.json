{
    "title": "SELECT dept, id WHERE id IS '*10' AND avg=90 ORDER BY dept",
    "query": {
        "WHERE": {
            "AND": [
                {
                "IS": {
                     "courses_id":"*10"
                 }
             },
             {
                "EQ": {
                    "courses_avg":90
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id",
                "courses_avg"
            ],
            "ORDER": "courses_dept"
        }
    },
